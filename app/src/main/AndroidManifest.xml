<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habebe.projecthomework">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_main" />
        <activity
            android:name=".activity.DataHomeWorkActivity"
            android:label="@string/app_nameDataHw" />
        <activity
            android:name=".activity.AddHomeworkActivity"
            android:label="@string/app_nameAddHw"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.AssignmentActivity"
            android:label="@string/label_assignment" />
        <activity
            android:name=".activity.ScheduleExamActivity"
            android:label="@string/label_schedule" />
        <activity
            android:name=".activity.ShowScoreActivity"
            android:label="@string/label_score" />
        <activity
            android:name=".activity.HistoryActivity"
            android:label="@string/label_history" />
        <activity
            android:name=".homework"
            android:label="@string/app_homework" />
        <activity android:name=".activity.DoExerciseActivity"
            android:label="Do Exercise"/><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>